\begin_layout Standard
Eine Menge 
\begin_inset Formula $\varPi$
\end_inset

 bildet einen Körper, wenn auf ihr die zweistelligen Operationen der Addition
 
\begin_inset Formula $+$
\end_inset

 und der Multiplikation 
\begin_inset Formula $\circ$
\end_inset

 definiert sind und die Eigenschaften 
\end_layout

\begin_layout Itemize
\begin_inset Formula $\left(\begin{array}{cc}
\varPi, & +\end{array}\right)$
\end_inset

 muss eine kommutative Gruppe mit dem neutralen Element 
\begin_inset Formula $e=0$
\end_inset

 sein, d.h.
 es muss gelten
\end_layout

\begin_deeper
\begin_layout Itemize
Assoziativgesetz: 
\begin_inset Formula $\forall a,b,c\in\varPi$
\end_inset

 gilt 
\begin_inset Formula $a+(b+c)=(a+b)+c$
\end_inset

.
\end_layout

\begin_layout Itemize
Distributivgesetz: 
\begin_inset Formula $\forall a,b\in\varPi$
\end_inset

 gilt 
\begin_inset Formula $a+b=b+a$
\end_inset

.
\end_layout

\begin_layout Itemize
Neutrales Element 
\begin_inset Formula $e$
\end_inset

: 
\begin_inset Formula $\forall a\in\varPi$
\end_inset

 gilt 
\begin_inset Formula $a+e=a$
\end_inset

.
\end_layout

\begin_layout Itemize
Inverses Element: 
\begin_inset Formula $\forall a\in\varPi$
\end_inset

 gibt es ein inverses Element 
\begin_inset Formula $a^{-1}$
\end_inset

 mit 
\begin_inset Formula $a+a^{-1}=e$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Itemize
\begin_inset Formula $\left(\begin{array}{cc}
\varPi\setminus\left\{ 0\right\} , & \circ\end{array}\right)$
\end_inset

 muss eine kommutative Gruppe mit dem neutralen Element 
\begin_inset Formula $e=1$
\end_inset

 sein, d.h.
 es muss gelten
\end_layout

\begin_deeper
\begin_layout Itemize
Assoziativgesetz: 
\begin_inset Formula $\forall a,b,c\in\varPi$
\end_inset

 gilt 
\begin_inset Formula $a\circ(b\circ c)=(a\circ b)\circ c$
\end_inset

.
\end_layout

\begin_layout Itemize
Distributivgesetz: 
\begin_inset Formula $\forall a,b\in\varPi$
\end_inset

 gilt 
\begin_inset Formula $a\circ b=b\circ a$
\end_inset

.
\end_layout

\begin_layout Itemize
Neutrales Element 
\begin_inset Formula $e$
\end_inset

: 
\begin_inset Formula $\forall a\in\varPi\setminus\left\{ 0\right\} $
\end_inset

 gilt 
\begin_inset Formula $a\circ e=a$
\end_inset

.
\end_layout

\begin_layout Itemize
Inverses Element: 
\begin_inset Formula $\forall a\in\varPi\setminus\left\{ 0\right\} $
\end_inset

 gibt es ein inverses Element 
\begin_inset Formula $a^{-1}$
\end_inset

 mit 
\begin_inset Formula $a\circ a^{-1}=e$
\end_inset

.
 
\end_layout

\end_deeper
\begin_layout Itemize
Distributivgesetze: 
\begin_inset Formula $\forall a,b,c\in\varPi$
\end_inset

 gilt 
\begin_inset Formula $a\circ\left(b+c\right)=a\circ b+a\circ c$
\end_inset

 und 
\begin_inset Formula $\left(a+b\right)\circ c=a\circ c+b\circ c$
\end_inset


\end_layout

